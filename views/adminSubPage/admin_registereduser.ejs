<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Edu Manage for Senate</title>
  <!-- Custom fonts for this template-->
  <link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="/css/css-main.css" rel="stylesheet">
  <script>
    function deleteAjax(user_email,course, level) {
      $.ajax({
        url: "/admin/deleteregistereduser",
        type: "POST",
        data: { user_email : user_email, course: course, level: level },
        success: function (data) {
          alert("user Name '" + user_email + "' Deleted!");
          window.location.href = '/admin/registereduser';
        }
      });
    }
  </script>
</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <% include ./admin_sidebar %>

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <% include ./admin_topbar %>
        <!-- Begin Page Content -->
        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">Registered User Information</h1>
          </div>
          <div class="row">
            <!-- Area Chart -->
            <div class="col-xl-12">
              <div class="card shadow mb-4">
                <!-- Card Header - Dropdown -->
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                  <h6 class="m-0 font-weight-bold text-primary">Basic course</h6>
                </div>
                <div class="card-body">
                  <table class="table table-striped table-bordered table-hover">
                    <thead style="font-size: 14px;">
                      <tr>
                        <th>Last-name</th>
                        <th>First-name</th>
                        <th>Sex</th>
                        <th>Date of birth</th>
                        <th>Nationality</th>
                        <th>Tel:</th>
                        <th>E-mail</th>                        
                        <th>Position</th>
                        <th>Department/Organization</th>
                        <th>Offical No:</th>
                        <th>Course selection</th>
                        <th>Delete</th>
                      </tr>
                    </thead>
                    <tbody style="font-size: 11px;">
                      <% if(coursesBasic == null || coursesBasic.length == 0){ %>
                      <tr class="noData">
                        <td colspan="13" style="text-align: center; font-size: 15px;">There is no user to show.</td>
                      </tr>
                      <% } %>
                      <% coursesBasic.forEach(function(coursesBasic){ %>
                      <tr>
                        <td><%= coursesBasic.user_fn %></td>
                        <td> <%= coursesBasic.user_gn %></td>
                        <td> <%= coursesBasic.user_sex %></td>
                        <td> <%= coursesBasic.user_birth %></td>
                        <td> <%= coursesBasic.user_nation %></td>
                        <td> <%= coursesBasic.user_phone %></td>
                        <td> <%= coursesBasic.user_email %></td>
                        <td> <%= coursesBasic.user_position %></td>
                        <td> <%= coursesBasic.user_part %></td>
                        <td> <%= coursesBasic.user_offid %></td>
                        <td> <%= coursesBasic.courses %></td>
                        
                        <td>
                          <a href="#" onclick="deleteAjax('<%= coursesBasic.user_email %>','<%= coursesBasic.courses %>', 'basic')" style="font-size: 11px;"
                            class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm float-right"><i
                              class="fas fa-plus"></i> Delete</a>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>

                  </table>
                </div>
                <!-- Card Body -->
              </div>
            </div>
            <!-- Pie Chart -->
          </div>


          <div class="row">
            <!-- Area Chart -->
            <div class="col-xl-12">
              <div class="card shadow mb-4">
                <!-- Card Header - Dropdown -->
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                  <h6 class="m-0 font-weight-bold text-primary">Advanced course</h6>
                </div>
                <div class="card-body">
                  <table class="table table-striped table-bordered table-hover">
                    <thead style="font-size: 14px;">
                      <tr>
                        <th>Last-name</th>
                        <th>First-name</th>
                        <th>Sex</th>
                        <th>Date of birth</th>
                        <th>Nationality</th>
                        <th>Tel:</th>
                        <th>E-mail</th>                        
                        <th>Position</th>
                        <th>Department/Organization</th>
                        <th>Offical No:</th>
                        <th>Course selection</th>
                         <th>Delete</th>
                      </tr>
                    </thead>
                    <tbody style="font-size: 11px;">
                      <% if(coursesAdvanced == null || coursesAdvanced.length == 0){ %>
                      <tr class="noData">
                        <td colspan="13" style="text-align: center; font-size: 15px;">There is no user to show.</td>
                      </tr>
                      <% } %>
                      <% coursesAdvanced.forEach(function(coursesAdvanced){ %>
                      <tr>
                        <td><%= coursesAdvanced.user_fn %></td>
                        <td> <%= coursesAdvanced.user_gn %></td>
                        <td> <%= coursesAdvanced.user_sex %></td>
                        <td> <%= coursesAdvanced.user_birth %></td>
                        <td> <%= coursesAdvanced.user_nation %></td>
                        <td> <%= coursesAdvanced.user_phone %></td>
                        <td> <%= coursesAdvanced.user_email %></td>
                        <td> <%= coursesAdvanced.user_position %></td>
                        <td> <%= coursesAdvanced.user_part %></td>
                        <td> <%= coursesAdvanced.user_offid %></td>
                        <td> <%= coursesAdvanced.courses %></td>
                        <td>
                          <a href="#" onclick="deleteAjax('<%= coursesAdvanced.user_email %>','<%= coursesAdvanced.courses %>', 'advanced')" style="font-size: 11px;"
                            style="font-size: 11px;"
                            class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm float-right"><i
                              class="fas fa-plus"></i>Delete</a>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>

                  </table>
                </div>
                <!-- Card Body -->
              </div>
            </div>
            <!-- Pie Chart -->
          </div>

          <div class="row">
            <!-- Area Chart -->
            <div class="col-xl-12">
              <div class="card shadow mb-4">
                <!-- Card Header - Dropdown -->
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                  <h6 class="m-0 font-weight-bold text-primary">Special lecture</h6>
                </div>
                <div class="card-body">
                  <table class="table table-striped table-bordered table-hover">
                    <thead style="font-size: 14px;">
                      <tr>
                        <th>Last-name</th>
                        <th>First-name</th>
                        <th>Sex</th>
                        <th>Date of birth</th>
                        <th>Nationality</th>
                        <th>Tel:</th>
                        <th>E-mail</th>                        
                        <th>Position</th>
                        <th>Department/Organization</th>
                        <th>Offical No:</th>
                        <th>Course selection</th>
                         <th>Delete</th>
                      </tr>
                    </thead>
                    <tbody style="font-size: 11px;">
                      <% if(coursesSpecial == null || coursesSpecial.length == 0){ %>
                      <tr class="noData">
                        <td colspan="13" style="text-align: center; font-size: 15px;">There is no user to show.</td>
                      </tr>
                      <% } %>
                      <% coursesSpecial.forEach(function(coursesSpecial){ %>
                      <tr>
                        
                        <td><%= coursesSpecial.user_fn %></td>
                        <td> <%= coursesSpecial.user_gn %></td>
                        <td> <%= coursesSpecial.user_sex %></td>
                        <td> <%= coursesSpecial.user_birth %></td>
                        <td> <%= coursesSpecial.user_nation %></td>
                        <td> <%= coursesSpecial.user_phone %></td>
                        <td> <%= coursesSpecial.user_email %></td>
                        <td> <%= coursesSpecial.user_position %></td>
                        <td> <%= coursesSpecial.user_part %></td>
                        <td> <%= coursesSpecial.user_offid %></td>
                        <td> <%= coursesSpecial.courses %></td>
                        
                        <td>
                          <a href="#" onclick="deleteAjax('<%= coursesSpecial.user_email %>','<%= coursesSpecial.courses %>', 'special')" style="font-size: 11px;"
                            style="font-size: 11px;"
                            class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm float-right"><i
                              class="fas fa-plus"></i> Delete</a>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>

                  </table>
                </div>
                <!-- Card Body -->
              </div>
            </div>
            <!-- Pie Chart -->

          </div>


        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      <footer class="sticky-footer bg-white">
        <div class="container my-auto">
          <div class="copyright text-center my-auto">
            <span>Copyright 2019. Senate of Cambodia</span>
          </div>
        </div>
      </footer>
      <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Bootstrap core JavaScript-->
  <script src="/vendor/jquery/jquery.min.js"></script>
  <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="/vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="/js/script-main.js"></script>
</body>

</html>